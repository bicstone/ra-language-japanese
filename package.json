{
  "name": "@bicstone/ra-language-japanese",
  "description": "Japanese messages for react-admin",
  "version": "3.0.0-beta.1",
  "author": "Takanori Oishi",
  "bugs": {
    "url": "https://github.com/bicstone/ra-language-japanese/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "files": [
    "LICENSE",
    "*.md",
    "lib",
    "src"
  ],
  "homepage": "https://github.com/bicstone/ra-language-japanese/",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/index.test.ts"
    ]
  },
  "keywords": [
    "admin-on-rest",
    "react",
    "react-admin",
    "translation"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bicstone/ra-language-japanese.git"
  },
  "scripts": {
    "build": "tsc ./src/index.ts -d --outDir lib",
    "format": "prettier --write ./src/index.ts",
    "test": "jest"
  },
  "sideEffects": false,
  "test": "jest"
}
